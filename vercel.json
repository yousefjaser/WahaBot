{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "index.html",
      "use": "@vercel/static"
    },
    {
      "src": "logo.png",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/logo.png",
      "dest": "/logo.png"
    },
    {
      "src": "/api",
      "dest": "/api/index.js"
    },
    {
      "src": "/docs(.*)",
      "status": 302,
      "headers": {
        "Location": "/"
      }
    },
    {
      "src": "/swagger(.*)",
      "status": 302,
      "headers": {
        "Location": "/"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
} 